{
  "name": "hypixel-guild-discord-bridge",
  "version": "4.0.0",
  "description": "A Hypixel guild chat and Discord chat bridge",
  "type": "module",
  "main": "index.ts",
  "engines": {
    "node": ">=22.16"
  },
  "scripts": {
    "start": "node --import tsx/esm index.ts",
    "generate-code": "npx ts-interface-builder src/application-config.ts",
    "generate-docs": "node --import tsx/esm scripts/generate-documentation.ts",
    "lint:fix": "npx eslint . --fix && npx prettier . \"!**/*.ts\" --write",
    "test:compile": "npx tsc --noEmit",
    "test:run": "node --import tsx/esm index.ts test-run",
    "validate": "npm install && npm run generate-code && npm run generate-docs && npm run lint:fix && npm run test:compile && npx type-coverage --detail true --update true && npm run test:run"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aidn3/hypixel-guild-discord-bridge.git"
  },
  "keywords": [
    "hypixel",
    "discord",
    "chat",
    "bridge"
  ],
  "author": "aidn5 (aidn.david3@gmail.com)",
  "typeCoverage": {
    "atLeast": 100
  },
  "dependencies": {
    "@hkh12/node-calc": "^4.2.1",
    "axios": "^1.11.0",
    "bad-words": "3.0.4",
    "better-sqlite3": "^12.2.0",
    "canvas": "^3.1.0",
    "deepcopy": "^2.1.0",
    "defaults": "^3.0.0",
    "discord.js": "^14.15.3",
    "emoji-name-map": "^2.0.1",
    "esrever": "^0.2.0",
    "fast-deep-equal": "^3.1.3",
    "hypixel-api-reborn": "^11.2.1",
    "log4js": "^6.9.1",
    "minecraft-data": "^3.66.0",
    "minecraft-protocol": "^1.47.0",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
    "prismarine-chat": "^1.10.1",
    "prismarine-registry": "^1.7.0",
    "prom-client": "^15.1.3",
    "promise-queue": "^2.2.5",
    "skyhelper-networth": "^2.0.0",
    "socks": "^2.8.3",
    "string-comparison": "^1.3.0",
    "tiny-typed-emitter": "^2.1.0",
    "ts-interface-checker": "^1.0.2",
    "tsx": "^4.16.2",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/esrever": "^0.2.3",
    "@types/promise-queue": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "discord-api-types": "^0.38.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.3.0",
    "markdown-table": "^3.0.4",
    "prettier": "^3.3.3",
    "prismarine-nbt": "^2.7.0",
    "ts-interface-builder": "^0.3.3",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.28.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
